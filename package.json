{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "React Webpack Boilerplate",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
